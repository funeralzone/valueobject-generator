{
  "name": "funeralzone/valueobject-generator",
  "description": "A generator for immutable Value Objects",
  "authors": [
    {
      "name": "James Sanders",
      "email": "james.sanders@funeralzone.co.uk"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "symfony/yaml": "3.*",
    "twig/twig": "2.4.*",
    "prettus/laravel-validation": "1.1.*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2",
    "squizlabs/php_codesniffer": "^3.0",
    "mockery/mockery": "^0.9.9"
  },
  "autoload": {
    "psr-4": {
      "Funeralzone\\ValueObjectGenerator\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  }
}
